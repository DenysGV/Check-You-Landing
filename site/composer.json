{
    "name": "robohata/site",
    "description": "Simple robohata site.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.10",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}